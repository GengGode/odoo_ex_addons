<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="provider_third" model="auth.oauth.provider">
            <field name="name">企业微信登录</field>
            <field name="enabled">1</field>
            <field name="auth_endpoint">https://i.calluu.cn/oauth2/auth</field>
            <field name="scope">userinfo</field>
            <field name="validation_endpoint">https://i.calluu.cn/oauth2/tokeninfo</field>
            <field name="data_endpoint"></field>
            <field name="body">企业微信登录</field>
        </record>
        <record id="weodoo_config_default" model="wo.config">
            <field name="oauth_client_key"></field>
            <field name="enable_wx_notify">1</field>
        </record>
    </data>
</odoo>
